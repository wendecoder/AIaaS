{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/UserPage", "src/app/ServiceModeal/FeatureModal.js", "src/app/ADAPriceChart/page.js"],
  "exclude": ["node_modules"],
  "types": ["@types/node"], // Add this line to include Node.js types
  "lib": ["dom", "dom.iterable", "esnext"],
  "typeRoots": ["./src/types"], // Include the path to your custom types directory
}
